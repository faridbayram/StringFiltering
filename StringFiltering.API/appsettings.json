{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Levenshtein": {
    "FilteringThreshold": 0.8,
    "FilterWords": [
      "bad-word-1",
      "bad-word-2",
      "etc."
    ]
  },
  "JaroWinkler": {
    "FilteringThreshold": 0.4,
    "FilterWords": [
      "bad-word-3",
      "bad-word-4",
      "etc."
    ]
  }
}
